{
	"compilerOptions": {
		"noEmit": false,
		"outDir": "../build",
        "rootDir": "./",
		"inlineSourceMap": false,
		"inlineSources": false,
		"sourceMap": true,
		"declaration": true,
		"noImplicitReturns": true,
		"noImplicitAny": true,
		"noEmitOnError": true,
		"removeComments": false,
		"target": "es5",
		"module": "commonjs",
		"allowSyntheticDefaultImports": true
	}
}
